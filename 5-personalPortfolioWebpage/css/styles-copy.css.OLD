/* ==========================================================================
   Base styles: opinionated defaults
   ========================================================================== */
html {
  /* switch to border-box model for all elements */
 box-sizing: border-box;
 /* set baseline font size to 10px */
 font-size: 62.5%;
 /* For IE11 to do the math properly
    See http://bit.ly/1g4X0bX
    thanks @7studio and @dawitti */
 font-size: calc(1em * .625);
}

* {
 box-sizing: inherit;
}

body {
  background-color: #fff;
  color: #4a4949;
  font-family: "Source Sans Pro", Helvetica, Arial, sans-serif;
  font-size: 1.6rem;
  line-height: 1.6;
}

a {
  text-decoration: none;
  color: #008cba;
}

a:focus, a:hover, a:active {
  color: #000;
}

h1,
.h1-like,
h2,
.h2-like,
h3,
.h3-like {
  font-family: Merriweather, Georgia, serif;
  font-weight: 400;
  letter-spacing: -0.25px;
}

h1:first-child,
.h1-like:first-child,
h2:first-child,
.h2-like:first-child,
h3:first-child,
.h3-like:first-child,
p:first-child {
  margin-top: 0;
}

h1,
.h1-like {
  font-size: 4rem;
}

h2,
.h2-like {
  font-size: 2.8rem;
}

p {
  margin-top: .75em;
  margin-bottom: 0;
  line-height: 1.6;
}

img {
  height: auto;
  max-width: 100%;
  border: 0;
  vertical-align: middle;
}

label {
  display: block;
  font-weight: 700;
  font-size: 1.8rem;
  color: #686868;
  vertical-align: middle;
  cursor: pointer;
}

ul {
  margin-top: .75em;
  margin-bottom: 0;
  line-height: 1.6;
}

.anchor {
  padding-top: 8rem;
}

.clearfix:before,
.clearfix:after {
    content: " ";
    display: table;
}

.btn {
  background-color: #128ad6;
  color: #fff;
  text-decoration: none;
  display: inline-block;
  padding: 0.5em 0.7em;
  font-weight: 600;
  border-radius: 4px;
  border: none;
  font-size: 1.6rem;
  cursor: pointer;
}

.btn:focus,
.btn:active,
.btn:hover {
  background-color: #1279ba;
  color: #fff;
}

.text-center {
  text-align: center;
}

/* ==========================================================================
   Header Section : Navbar
   ========================================================================== */
 header {
   width: 100%;
   position: fixed;
   padding: 1.5em;
   top: 0;
   right: 0;
   background-color: rgba(255, 255, 255, 1);
   z-index: 1
   text-align: center;
   border-bottom: 2px solid rgba(238, 238, 238, 1);
 }

 header a {
   color: #555;
   text-decoration: none;
   padding: .5em 0.7em;
   font-weight: 300;
   margin-left: 10px;
   display: inline-block;
 }

header .logo {
  font-size: 1.6rem;
  float: left;
  text-transform: uppercase;
  margin-right: 2em;
  letter-spacing: 1px;
}

header nav {
  margin-right: 20px;
  text-align: right;
}

header nav .btn {
  font-weight: 300;
}

header nav .nav-links:focus
header .logo:focus,
header nav .nav-links:hover,
header .logo:hover,
header nav .nav-links:active,
header .logo:hover {
  color: #128ad6;
}

/* ==========================================================================
   Main Section : welcome
   ========================================================================== */
main {
}

#welcome-section {
  display: flex;
  justify-content: center;
  min-height: 100vh;
  padding-top: 50px;
  background: #fff url(./images/bg-cover.jpg) no-repeat center fixed;
  -webkit-background-size: cover; /* pour anciens Chrome et Safari */
  background-size: cover;
}

#welcome-section > .card {
  max-width: 90%;
  margin: auto;
  padding: 50px;
  background-color: rgba(238, 238, 238, 0.8);
}

#welcome-section > .card .card-title {
  margin: 0;
  font-size: 2.7rem;
  font-weight: 600;
  color: #128ad6;
}

#welcome-section > .card .card-subtitile {
  margin-top: 0;
  font-weight: 300;
  font-size: 1.6rem;
  text-transform: uppercase;
}

#welcome-section > .card .card-text {
  margin-bottom: 2em;
}

/* ==========================================================================
   Main Section : expertises & projects
   ========================================================================== */
.mosaic {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 4rem 2.5%;
  flex: 0 0 auto;;
}

.expertise,
.project {
  display: flex;
  margin-bottom: 2rem;
  width: calc(100% * 1 / 3 - 1em - .01px);
}

.expertise >a,
.project > a {
  display: flex;
  overflow: hidden;
  flex-direction: column;
  border: 1px solid #c0c6ca
}

.expertise,
.project-info {
  display: flex;
  flex-direction: column;
  background: #fff;
  padding: 1rem 1rem 2rem;
  text-align: center;
}

.expertise-title,
.project-title {
  color: #008cba;
  font-size: 2rem;
}

a:focus .project-title,
.project-title:focus,
a:hover .project-title,
.project-title:hover {
  color: #f25e23;
}

.project-tags {
  overflow: hidden;
  margin: 0;
  padding: 0;
  color: #828282;
}

.project-tags > li {
  display: inline-block;
  font-style: italic;
}

.project-tags > li + li {
  margin-left: 1rem;
}

.expertise-text,
.project-text {
  margin-top: 1rem;
  padding: 0 2em;
  font-size: 1.5rem;
  text-align: left;
  color: #232323;
}

.expertise-link {
  flex-grow: 1;
  display: flex;
  justify-content: center;
  margin-top: 2em;
}

.expertise-link > a {
  align-self: flex-end;
}

/* ==========================================================================
   Main Section : contact
   ========================================================================== */
.form-item {
  margin-top: 0;
  margin-bottom: .5rem;
}

.form-text {
  width: 100%;
  padding: 1em;
  border: 2px solid #c1c1c0;
  color: #4a4949;
  margin-top: .3em;
  border-right: 0;
}
@media screen and (min-width: 1281px) {
  #expertise-section,
  #projetcs,
  #contacts {
      padding: 8rem 13.5%;
  }
}

@media screen and (max-width: 1025px) {
  #expertise-section,
  #projects,
  #contacts {
      padding: 8rem 7%;
  }
  .expertise {
    width: calc(100% * 1 / 2 - 1em - .01px);
  }

  .contact {
    width: calc(100% - 1em - .01px);
  }
}

@media screen and (max-width: 768px) {
  body {
    font-size: 1.4em;
    line-height: 1.4;
  }
  .expertise {
    width: calc(100% - 1em - .01px);
  }
  .project {
    width: calc(100% * 1 / 2 - 1em - .01px);
  }
}

@media screen and (max-width: 480px) {
  #expertise-section,
  .mosaic {
    padding: 4rem 5%;
  }
  .expertise,
  .project {
    width: calc(100% - 1em - .01px);
  }
}
